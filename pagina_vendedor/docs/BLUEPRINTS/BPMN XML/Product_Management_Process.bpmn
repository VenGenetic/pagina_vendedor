<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" id="Definitions_ProductMgmt" targetNamespace="http://menards.com/bpmn/product" exporter="Camunda Modeler" exporterVersion="5.43.1">
  <bpmn:collaboration id="Collaboration_ProductMgmt">
    <bpmn:participant id="Participant_ProductMgmt" name="Product Management (Metadata Only)" processRef="Process_ProductMgmt" />
  </bpmn:collaboration>
  <bpmn:process id="Process_ProductMgmt" name="Product Management Operations" isExecutable="true">
    <bpmn:laneSet id="LaneSet_ProductMgmt">
      <bpmn:lane id="Lane_SystemServices" name="System Services / Servicios del Sistema">
        <bpmn:flowNodeRef>Activity_ValidateNewProduct</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_CreateProduct</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_RecordOpeningEquity</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_InjectStock</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_ValidateCostMargin</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_CostMarginCheck</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_UpdatePriceMetadata</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_PriceUpdated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_PriceRejected</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_AdminUI" name="Admin / User (UI) / Usuario (UI)">
        <bpmn:flowNodeRef>StartEvent_AddProduct</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_EnterProductDetails</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ProductValid</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_EnterOpeningBalance</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>StartEvent_PriceUpdate</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_RequestPriceUpdate</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_ProductRejected</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_ProductCreated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_HasInitialStock</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_StagingReview</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_AddProduct" name="Add Product Requested / Solicitud Agregar Producto">
      <bpmn:outgoing>Flow_AP1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Activity_EnterProductDetails" name="Enter Product Details / Ingresar Detalles del Producto">
      <bpmn:documentation>Form to enter code, name, category, initial list price.
      NO STOCK is added here. Current Stock = 0.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="code" label="Product Code" type="string" />
          <camunda:formField id="name" label="Product Name" type="string" />
          <camunda:formField id="listPrice" label="List Price" type="long" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AP1</bpmn:incoming>
      <bpmn:outgoing>Flow_AP2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Activity_ValidateNewProduct" name="Validate New Product / Validar Nuevo Producto" camunda:type="external" camunda:topic="product-validate-new">
      <bpmn:documentation>Checks if Code is unique.
Check price rounding rules.</bpmn:documentation>
      <bpmn:incoming>Flow_AP2</bpmn:incoming>
      <bpmn:outgoing>Flow_AP3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_ProductValid" name="Valid? / ¿Válido?">
      <bpmn:incoming>Flow_AP3</bpmn:incoming>
      <bpmn:outgoing>Flow_ProdYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_ProdNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Activity_CreateProduct" name="Create Product Metadata / Crear Metadatos Producto" camunda:type="external" camunda:topic="product-create-metadata">
      <bpmn:documentation>Inserts row into 'products' table.
Set current_stock = 0 initially.</bpmn:documentation>
      <bpmn:incoming>Flow_ProdYes</bpmn:incoming>
      <bpmn:outgoing>Flow_AP4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Activity_EnterOpeningBalance" name="Enter Opening Balance / Ingresar Saldo Inicial">
      <bpmn:documentation>User enters Opening Balance for products already on shelf.
CRITICAL: Requires both Quantity AND Unit Cost for proper WAC initialization and financial tracking.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="openingQuantity" label="Opening Quantity" type="long" defaultValue="0" />
          <camunda:formField id="unitCost" label="Unit Cost (WAC)" type="long" defaultValue="0">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="min" config="0" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="adjustmentReason" label="Reason" type="string" defaultValue="Opening Balance" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AP4</bpmn:incoming>
      <bpmn:outgoing>Flow_StockCheck</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Activity_RecordOpeningEquity" name="Record Opening Balance Equity / Registrar Capital Inicial" camunda:type="external" camunda:topic="finance-record-opening-balance">
      <bpmn:documentation>Posts Journal Entry:
  Debit: Inventory Asset (openingQuantity * unitCost)
  Credit: Opening Balance Equity
This ensures assets match the accounting equation and enables audit compliance.</bpmn:documentation>
      <bpmn:incoming>Flow_StockYes</bpmn:incoming>
      <bpmn:outgoing>Flow_ToInjectStock</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_InjectStock" name="Create Opening Movement / Crear Movimiento Inicial" camunda:type="external" camunda:topic="inventory-create-movement">
      <bpmn:documentation>Creates inventory_movement record with:
  - movementType: 'OPENING_BALANCE'
  - quantity: openingQuantity
  - unitCost: unitCost
Updates product: current_stock = openingQuantity, wac = unitCost</bpmn:documentation>
      <bpmn:incoming>Flow_ToInjectStock</bpmn:incoming>
      <bpmn:outgoing>Flow_ToEndFromStock</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:startEvent id="StartEvent_PriceUpdate" name="Price Update Requested / Solicitud Actualizar Precio">
      <bpmn:outgoing>Flow_PU1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Activity_RequestPriceUpdate" name="Request Price Update / Solicitar Cambio de Precios">
      <bpmn:documentation>Triggered by 'Actualización de Precios' button.
Allows changing List Price. Selling Price is automatically Cost * 1.65.</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="productId" label="Product ID" type="string" />
          <camunda:formField id="newListPrice" label="New List Price" type="long" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_PU1</bpmn:incoming>
      <bpmn:outgoing>Flow_PU2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Activity_ValidateCostMargin" name="Validate Margin &#38; Cost / Validar Margen y Costo" camunda:type="external" camunda:topic="pricing-validate-cost-margin">
      <bpmn:documentation>Fetches current WAC from inventory.
Rejects if NewPrice &lt; WAC.
Ensures Margin requirements are met.</bpmn:documentation>
      <bpmn:incoming>Flow_PU2</bpmn:incoming>
      <bpmn:outgoing>Flow_PU3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_CostMarginCheck" name="Valid Price? / ¿Precio Válido?">
      <bpmn:incoming>Flow_PU3</bpmn:incoming>
      <bpmn:outgoing>Flow_PriceYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_PriceNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Activity_UpdatePriceMetadata" name="Update Price Metadata / Actualizar Metadatos Precio" camunda:type="external" camunda:topic="pricing-update-metadata">
      <bpmn:documentation>Updates product row.
NO CONNECTION to Financial or Stock Ledger.</bpmn:documentation>
      <bpmn:incoming>Flow_ToUpdate</bpmn:incoming>
      <bpmn:outgoing>Flow_PU4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_PriceUpdated" name="Price Updated Successfully / Precio Actualizado">
      <bpmn:incoming>Flow_PU4</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_ProductRejected" name="Product Rejected / Producto Rechazado">
      <bpmn:incoming>Flow_ProdNo</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_ProductCreated" name="Product Added / Producto Agregado">
      <bpmn:incoming>Flow_StockNo</bpmn:incoming>
      <bpmn:incoming>Flow_ToEndFromStock</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:exclusiveGateway id="Gateway_HasInitialStock" name="Has Opening Stock? / ¿Tiene Stock Inicial?">
      <bpmn:incoming>Flow_StockCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_StockYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_StockNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_PriceRejected" name="Price Rejected (Below Cost) / Precio Rechazado">
      <bpmn:incoming>Flow_PriceNo</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_AP1" sourceRef="StartEvent_AddProduct" targetRef="Activity_EnterProductDetails" />
    <bpmn:sequenceFlow id="Flow_AP2" sourceRef="Activity_EnterProductDetails" targetRef="Activity_ValidateNewProduct" />
    <bpmn:sequenceFlow id="Flow_AP3" sourceRef="Activity_ValidateNewProduct" targetRef="Gateway_ProductValid" />
    <bpmn:sequenceFlow id="Flow_ProdYes" name="Yes" sourceRef="Gateway_ProductValid" targetRef="Activity_CreateProduct">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isValid == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_ProdNo" name="No" sourceRef="Gateway_ProductValid" targetRef="EndEvent_ProductRejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isValid == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_AP4" sourceRef="Activity_CreateProduct" targetRef="Activity_EnterOpeningBalance" />
    <bpmn:sequenceFlow id="Flow_StockCheck" sourceRef="Activity_EnterOpeningBalance" targetRef="Gateway_HasInitialStock" />
    <bpmn:sequenceFlow id="Flow_StockYes" name="Yes" sourceRef="Gateway_HasInitialStock" targetRef="Activity_RecordOpeningEquity">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${openingQuantity &gt; 0}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_ToInjectStock" sourceRef="Activity_RecordOpeningEquity" targetRef="Activity_InjectStock" />
    <bpmn:sequenceFlow id="Flow_ToEndFromStock" sourceRef="Activity_InjectStock" targetRef="EndEvent_ProductCreated" />
    <bpmn:sequenceFlow id="Flow_PU1" sourceRef="StartEvent_PriceUpdate" targetRef="Activity_RequestPriceUpdate" />
    <bpmn:sequenceFlow id="Flow_PU2" sourceRef="Activity_RequestPriceUpdate" targetRef="Activity_ValidateCostMargin" />
    <bpmn:sequenceFlow id="Flow_PU3" sourceRef="Activity_ValidateCostMargin" targetRef="Gateway_CostMarginCheck" />
    <bpmn:sequenceFlow id="Flow_PriceYes" name="Yes" sourceRef="Gateway_CostMarginCheck" targetRef="Activity_StagingReview">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isValid == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_PriceNo" name="No" sourceRef="Gateway_CostMarginCheck" targetRef="EndEvent_PriceRejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isValid == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_PU4" sourceRef="Activity_UpdatePriceMetadata" targetRef="EndEvent_PriceUpdated" />
    <bpmn:sequenceFlow id="Flow_StockNo" name="No" sourceRef="Gateway_HasInitialStock" targetRef="EndEvent_ProductCreated">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${openingQuantity == 0}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_ToUpdate" sourceRef="Activity_StagingReview" targetRef="Activity_UpdatePriceMetadata" />
    <bpmn:userTask id="Activity_StagingReview" name="Metadata Alignment Staging / Alineación de Metadatos">
      <bpmn:documentation>Unified Data Alignment Interface:
      1. Load Current Inventory: Fetches existing products to the staging area.
      2. Download Template: Generates CSV from current inventory or example.
      3. Upload CSV: Parses data and matches against DB using SKUs.
      4. Staging Review: Visual grid highlighting price discrepancies.
      5. Safe Commit: Uses RPC 'process_metadata_alignment' to update metadata without touching stock.
      
      PRICE PROPOSAL QUEUE:
      - Additional source of Price Alignment requests.
      - Generated by 'Restock Process' when cost changes are detected.
      - Requires approval via 'approve_price_proposal' RPC.</bpmn:documentation>
      <bpmn:incoming>Flow_PriceYes</bpmn:incoming>
      <bpmn:outgoing>Flow_ToUpdate</bpmn:outgoing>
    </bpmn:userTask>
  </bpmn:process>
